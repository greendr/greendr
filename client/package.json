{
  "name": "react-client-server-starter-client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:4000/",
  "dependencies": {
    "@types/interact.js": "^1.0.22",
    "@types/jest": "^22.2.0",
    "@types/lodash": "^4.14.55",
    "@types/node": "^9.4.7",
    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "@types/react-router-dom": "^4.2.5",
    "@types/webfontloader": "^1.6.29",
    "interact.js": "^1.2.8",
    "mobx": "^5.0.2",
    "mobx-react": "^5.2.3",
    "moment": "^2.22.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-md": "^1.4.0",
    "react-router": "^4.2.0",
    "react-router-dom": "next",
    "react-scripts-ts": "^2.16.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "typescript": "^2.8.3",
    "webfontloader": "^1.6.28",
    "xhr-request": "^1.1.0"
  },
  "scripts": {
    "build-css": "node-sass --include-path ./node_modules src/ -o src",
    "watch-css": "yarn run build-css && node-sass --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts-ts start",
    "start": "npm-run-all -p watch-css start-js",
    "lint": "tslint src",
    "build": "yarn run build-css && react-scripts-ts build && rm -rf ../build/static && cp -rf build ../",
    "coverage": "react-scripts-ts test --env=jsdom --coverage --collectCoverageFrom=src/**/*.{ts,tsx} --collectCoverageFrom=!src/**/*.d.ts --collectCoverageFrom=!src/tests/**",
    "test": "react-scripts-ts test --env=jsdom"
  }
}
